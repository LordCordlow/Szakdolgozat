<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="840.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jchess.view.controller.MatchesController">
   <children>
      <TableView fx:id="matchesTable" layoutY="48.0" prefHeight="462.0" prefWidth="830.0" visible="false">
        <columns>
          <TableColumn fx:id="whitePlayerCol" prefWidth="160.0" text="White player" />
          <TableColumn fx:id="blackPlayerCol" prefWidth="160.0" text="Black player" />
            <TableColumn fx:id="winnerCol" prefWidth="160.0" text="Winner" />
            <TableColumn fx:id="startCol" prefWidth="160.0" text="Match start" />
            <TableColumn fx:id="endCol" prefWidth="169.0" text="Match end" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onBack" text="Back" />
      <Button fx:id="switchBtn" layoutX="735.0" layoutY="14.0" mnemonicParsing="false" onAction="#onSwitch" text="Switch" />
      <TableView fx:id="tandemsTable" layoutY="58.0" prefHeight="452.0" prefWidth="830.0">
         <columns>
            <TableColumn fx:id="hostTeamCol" prefWidth="160.0" text="Green team" />
            <TableColumn fx:id="oppTeamCol" prefWidth="160.0" text="Red team" />
            <TableColumn fx:id="tandemWinnerCol" prefWidth="160.0" text="Winner" />
            <TableColumn fx:id="tandemStartCol" prefWidth="160.0" text="Match start" />
            <TableColumn fx:id="tandemEndCol" prefWidth="179.0" text="Match end" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
